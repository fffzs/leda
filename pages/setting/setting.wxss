.setview{
  background-color: #00ffff;
  height: 50px;
  margin: 5px;
}
.setleftimg{
  width: 50px;
  height: 50px;
}
.setrightimg{
  width: 50px;
  height: 50px;
  position: absolute;
  right: 0px;
}
